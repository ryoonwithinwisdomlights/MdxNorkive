{
  "name": "@norkive/mdx-cloudinary-processor",
  "version": "1.0.0",
  "description": "MDX content media processor with Cloudinary integration. Process images and documents from Notion to Cloudinary.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mdx",
    "media",
    "processor",
    "image",
    "document",
    "cloudinary",
    "notion",
    "upload",
    "dependency-injection",
    "typescript"
  ],
  "author": "ryoon.with.wisdomtrees@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryoonwithinwisdomlights/MdxNorkive.git",
    "directory": "packages/mdx-cloudinary-processor"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "cloudinary": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "cloudinary": {
      "optional": false
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

